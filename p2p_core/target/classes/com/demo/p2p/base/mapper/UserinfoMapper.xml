  <insert id="insert">
    insert into `userinfo` (id,version, bitState, realname,idnumber, phonenumber, incomegrade_id,email,authscore,realauthId,
      marriage_id, kidcount_id, educationbackground_id,housecondition_id)
    values (#{id},0, 0, #{realname,jdbcType=VARCHAR}, 
      #{idnumber,jdbcType=VARCHAR}, #{phonenumber,jdbcType=VARCHAR}, #{incomegradeId,jdbcType=BIGINT},#{email},#{authscore},#{realauthid},
      #{marriageId,jdbcType=BIGINT}, #{kidcountId,jdbcType=BIGINT}, #{educationbackgroundId,jdbcType=BIGINT}, 
      #{houseconditionId,jdbcType=BIGINT})
  </insert>
  
  <update id="updateByPrimaryKey">
    update `userinfo`
    set version = version+1,
      bitState = #{bitState,jdbcType=BIGINT},
      realName = #{realName,jdbcType=VARCHAR},
      idNumber = #{idNumber,jdbcType=VARCHAR},
      email = #{email,jdbcType=VARCHAR},
      realAuthId = #{realauthid},
      authScore = #{authScore},
      phoneNumber = #{phoneNumber,jdbcType=VARCHAR},
      incomeGrade_id = #{incomegradeId,jdbcType=BIGINT},
      marriage_id = #{marriageId,jdbcType=BIGINT},
      kidCount_id = #{kidcountId,jdbcType=BIGINT},
      educationBackgroundId = #{educationbackground_id,jdbcType=BIGINT},
      houseConditionIid = #{housecondition_id,jdbcType=BIGINT}
    where id = #{id,jdbcType=BIGINT} AND version = #{version}
  </update>
  
  <sql id="base_column">
  	id, version, bitState, realName, idNumber, phoneNumber, incomeGrade_id, marriage_id,email,authScore,realAuthId,
    kidCount_id, educationBackground_id, houseCondition_id
  </sql>
  
  <select id="selectByPrimaryKey" resultMap="BaseResultMap">
    select <include refid="base_column" />
    from `userinfo`
    where id = #{id,jdbcType=BIGINT}
  </select>
  
